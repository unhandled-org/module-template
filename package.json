{
  "name": "un-nm-module",
  "version": "0.0.1",
  "description": "Node module template",
  "main": "index.js",
  "scripts": {
  "start": "node index.js",
  "example": "NODE_ENV=development node example.js",
  "test": "NODE_ENV=test mocha 'test/**/*.test.js' -r 'test/setup.js' --exit",
  "test:watch": "NODE_ENV=test mocha 'test/**/*.test.js' -r 'test/setup.js' --watch --reporter min",
  "test:cover": "NODE_ENV=test nyc mocha 'test/**/*.test.js' -r 'test/setup.js' --exit",
  "lint": "eslint \"**/*.js\" --ignore-pattern node_modules"
  },
  "engines": {
    "node": ">= 6.10",
    "npm": ">= 3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unhandled-org/un-nm-temmplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
  "url": "https://github.com/unhandled-org/un-nm-temmplate/issues"
  },
  "homepage": "https://github.com/unhandled-org/un-nm-temmplate#readme",
  "dependencies": {
    "dotenv": "^6.0.0",
    "joi": "^13.6.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "sinon": "^6.3.4",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1"
  }
}
